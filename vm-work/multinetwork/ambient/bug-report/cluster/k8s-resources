      sidecar.istio.io/inject: "false"
    name: ztunnel-w7skd
    namespace: istio-system
    ownerReferences:
    - apiVersion: apps/v1
      blockOwnerDeletion: true
      controller: true
      kind: DaemonSet
      name: ztunnel
      uid: ab42f261-8b74-4838-9099-34c01dd57ba3
    resourceVersion: "3812"
    uid: 63837726-403e-4484-b07e-b38767ad48d1
  spec:
    affinity:
      nodeAffinity:
        requiredDuringSchedulingIgnoredDuringExecution:
          nodeSelectorTerms:
          - matchFields:
            - key: metadata.name
              operator: In
              values:
              - gke-daniel-ambient-default-pool-d5dcc773-d4df
    containers:
    - args:
      - proxy
      - sidecar
      env:
      - name: ISTIO_META_GENERATOR
        value: ztunnel-envoy
      - name: ISTIO_META_AMBIENT_TYPE
        value: ztunnel
      - name: ISTIO_META_CLUSTER_ID
