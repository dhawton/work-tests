        value: "true"
      image: dhawton/proxyv2:ambient
      imagePullPolicy: IfNotPresent
      name: istio-proxy
      readinessProbe:
        failureThreshold: 3
        httpGet:
          path: /healthz/ready
          port: 15020
          scheme: HTTP
        periodSeconds: 10
        successThreshold: 1
        timeoutSeconds: 1
      resources: {}
