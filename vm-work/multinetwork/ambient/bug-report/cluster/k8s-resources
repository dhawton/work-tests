          Time_Format %Y-%m-%dT%H:%M:%S.%L%z

      [PARSER]
          Name        containerd
          Format      regex
          # The timestamp is described in https://www.rfc-editor.org/rfc/rfc3339#section-5.6
          Regex       ^(?<time>[0-9]{4}-[0-9]{2}-[0-9]{2}[Tt ][0-9]{2}:[0-9]{2}:[0-9]{2}(?:\.[0-9]+)?(?:[Zz]|[+-][0-9]{2}:[0-9]{2})) (?<stream>stdout|stderr) [^ ]* (?<log>.*)$
          Time_Key    time
          Time_Format %Y-%m-%dT%H:%M:%S.%L%z

