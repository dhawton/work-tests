                              type: object
                            weight:
                              description: Weight specifies the relative proportion
                                of traffic to be forwarded to the destination.
                              format: int32
                              type: integer
                          type: object
                        type: array
                      timeout:
                        description: Timeout for HTTP requests, default is disabled.
                        type: string
                    type: object
                  type: array
